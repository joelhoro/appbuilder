<Window x:Class="AppRunner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:AppRunner.Controls"
        Title="MainWindow" Height="708" Width="1255.667">
    <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_Save settings" Click="MenuItemClick" Tag="Save"/>
            </Menu>
            <controls:ApplicationListControl x:Name="AppListControl"></controls:ApplicationListControl>
            <StackPanel Orientation="Horizontal">
                <StackPanel Orientation="Vertical">
                    <Button Content="Build"></Button>
                    <Button Content="Run"></Button>
                    <Button Content="Build &amp; Run"></Button>
                </StackPanel>
            </StackPanel>
        
        <!--<myControl:UserControl1 x:Name="mycontrol" ></myControl:UserControl1>-->
        </DockPanel>
</Window>
